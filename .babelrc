{
  "presets": [
    "react", "es2015", "es2017", "stage-0", "stage-1"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-decorators-legacy",
    "jsx-control-statements",
    "transform-object-assign",
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-runtime"
  ]
}
